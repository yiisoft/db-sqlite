{
    "name": "yiisoft/db-sqlite",
    "type": "library",
    "description": "Yii DataBase SQLite Extension",
    "keywords": [
        "yii",
        "sqlite"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/db-sqlite",
        "issues": "https://github.com/yiisoft/db-sqlite/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ext-pdo_sqlite": "^7.4",
        "php": "^7.4",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/strings": "^3.0@dev"
    },
    "require-dev": {
        "phan/phan": "^3.0",
        "phpunit/phpunit": "^9.2.0",
        "yiisoft/aliases": "^1.0",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "yiisoft/log-target-file": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/json": "^3.0@dev"

    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Db\\Sqlite\\": "src",
            "Yiisoft\\Db\\Sqlite\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "tests": [
                "$common",
                "config/tests.php"
            ]
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "config": {
        "sort-packages": true
    }
}
